    "awesome-typescript-loader": "^3.4.1",
    "babel-loader": "8.0.0-beta.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.4",
    "html-webpack-plugin": "^2.30.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "script-loader": "^0.7.2",
    "webpack": "^3.10.0"
  }
}
